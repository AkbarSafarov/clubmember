<div class="form-popup-wrapper coupon_popup">
    <div class="formExtraWrapper">
        <div class="close-form"></div>
        <div class="form_block">
            <div class="title">Create new coupon</div>
            <form action="">
                <div class="field_row column-4">
                    <div class="field">
                        <div class="field_name">Discount percentage</div>
                        <input type="text" name="percentage" placeholder="Enter percent %">
                    </div>
                    <div class="field">
                        <div class="field_name">Discount amount</div>
                        <input type="text" name="discount-amount" placeholder="Enter amount">
                    </div>
                    <div class="field">
                        <div class="field_name">Expires in</div>
                        <input type="text" name="expires-in" placeholder="7 days" value="7 days">
                    </div>
                    <div class="field">
                        <div class="field_name">Number of guests</div>
                        <input type="text" name="guests" placeholder="4 people" value="4 people">
                    </div>
                </div>
                <div class="form_btns">
                    <button class="btn_button border cancel">Discard</button>
                    <button type="submit" class="btn_button">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="alert-popup-wrapper deactivate_popup">
