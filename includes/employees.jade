<div class="form-popup-wrapper employee_popup">
    <div class="formExtraWrapper">
        <div class="close-form"></div>
        <div class="form_block">
            <div class="title">Create new employee</div>
            <form action="">
                <div class="field_row column-2">
                    <div class="field">
                        <div class="field_name">First name</div>
                        <input type="text" name="first-name" placeholder="Enter first name">
                    </div>
                    <div class="field">
                        <div class="field_name">Last name</div>
                        <input type="text" name="last-name" placeholder="Enter last name">
                    </div>
                    <div class="field">
                        <div class="field_name">Phone number</div>
                        <input type="text" name="phone-number" placeholder="+998">
                    </div>
                    <div class="field">
                        <div class="field_name">Password</div>
                        <input type="text" name="password" placeholder="password" value="">
                    </div>
                    <div class="field">
                        <div class="field_name">Account type</div>
                        <select name="account-type" id="">
                            <option value="">Select from the list</option>
                            <option value="Waiter">Waiter</option>
                            <option value="Cashier">Cashier</option>
                            <option value="Administrator">Administrator</option>
                        </select>
                    </div>
                    <div class="field">
                        <div class="field_name">Date of birth</div>
                        <input type="text" name="date-birth" placeholder="DD/MM/YYYY" value="">
                    </div>
                </div>
                <div class="form_btns">
                    <button class="btn_button border cancel">Discard</button>
                    <button type="submit" class="btn_button">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="alert-popup-wrapper deactivate_popup">
    <div class="alertExtraWrapper">
        <div class="alert_block">
            <div class="icon"></div>
            <div class="title">You are going to deactivate account <br>+(998) 90 819-12-90, are you sure?</div>
            <div class="btns">
                <button class="btn_button border cancel">No, cancel</button>
                <button type="submit" class="btn_button red">Yes, deactivate</button>
            </div>
        </div>
    </div>
</div>
<div class="alert-popup-wrapper delete_popup">
    <div class="alertExtraWrapper">
        <div class="alert_block">
            <div class="icon"></div>
            <div class="title">You are going to delete account <br>+(998) 90 819-12-90, are you sure?</div>
            <div class="btns">
                <button class="btn_button border cancel">No, cancel</button>
                <button type="submit" class="btn_button red">Yes, delete</button>
            </div>
        </div>
    </div>
</div>
<div class="notifications_alert_fixed">
    <div class="notifications_alert successfully opened">
        <div class="note">Data saved successfully</div>
        <div class="close"></div>
    </div>
    <div class="notifications_alert error opened">
        <div class="note">Something went wrong</div>
        <div class="close"></div>
    </div>
</div>
