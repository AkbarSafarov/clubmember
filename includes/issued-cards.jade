<div class="form-popup-wrapper membership_popup">
    <div class="formExtraWrapper">
        <div class="close-form"></div>
        <div class="form_block">
            <div class="title">Create new membership card</div>
            <form action="">
                <div class="field_row column-2">
                    <div class="field">
                        <div class="field_name">User ID</div>
                        <input type="text" name="user-id" placeholder="Enter user ID">
                    </div>
                    <div class="field">
                        <div class="field_name">Expire Period</div>
                        <input type="text" name="expire-period" placeholder="Number of days">
                    </div>
                </div>
                <div class="field">
                    <div class="field_name">Type of cards</div>
                    <select name="cart-type" id="">
                        <option value="">Select from the list</option>
                        <option value="Cashback">Cashback card</option>
                        <option value="Discount">Discount card</option>
                        <option value="Membership">Membership card</option>
                        <option value="Private">Private card</option>
                    </select>
                </div>
                <div class="form_btns">
                    <button class="btn_button border">Discard</button>
                    <button type="submit" class="btn_button">Save</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="alert-popup-wrapper deactivate_popup">
